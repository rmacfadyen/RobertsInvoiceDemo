{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "module": "None",
    "outFile": "site.js"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "compileOnSave": true,
  "typingOptions": {
    "enableAutoDiscovery": false
  }
}
